module Lab4_1_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [3:0] SW;
// wires                                               
wire [0:6] HEX0;
wire [0:6] HEX1;

wire sampler;                             

// assign statements (if any)                          
Lab4_1 i1 (
// port map - connection between master ports and signals/registers   
	.HEX0(HEX0),
	.HEX1(HEX1),
	.SW(SW)
);
// SW[ 3 ]
initial
begin
	repeat(6)
	begin
		SW[3] = 1'b0;
		SW[3] = #80000 1'b1;
		# 80000;
	end
	SW[3] = 1'b0;
end 
// SW[ 2 ]
initial
begin
	repeat(12)
	begin
		SW[2] = 1'b0;
		SW[2] = #40000 1'b1;
		# 40000;
	end
	SW[2] = 1'b0;
end 
// SW[ 1 ]
always
begin
	SW[1] = 1'b0;
	SW[1] = #20000 1'b1;
	#20000;
end 
// SW[ 0 ]
always
begin
	SW[0] = 1'b0;
	SW[0] = #10000 1'b1;
	#10000;
end 

Lab4_1_vlg_sample_tst tb_sample (
	.SW(SW),
	.sampler_tx(sampler)
);

Lab4_1_vlg_check_tst tb_out(
	.HEX0(HEX0),
	.HEX1(HEX1),
	.sampler_rx(sampler)
);
endmodule

